# BuscaCursos UC API - Environment Variables
# Copy this file to .env and fill in the values

# Base URL for BuscaCursos UC
BUSCACURSOS_BASE_URL=https://buscacursos.uc.cl/

# Cache TTL in seconds (default: 300 = 5 minutes)
CACHE_TTL_SECONDS=300

# CORS allowed origins (comma-separated, use * for all)
ALLOWED_ORIGINS=*

# Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Environment (development, production)
ENVIRONMENT=development

# ScrapingAnt API Key (Required for production scraping)
SCRAPINGANT_API_KEY=your_key_here
